<h1>Users List</h1>

<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.name, user_path(user) %></li>
  <% end %>
</ul>

<%= javascript_tag type: "application/javascript" do %>
  document.addEventListener("DOMContentLoaded", function() {
    alert("Welcome to the Users Page!");
  });
<% end %>
